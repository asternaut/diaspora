  {%- assign wholesale = false -%}

  {%- for tag in customer.tags -%}
  	{%- assign downcased_tag = tag | downcase -%}
  	{%- if downcased_tag == "wholesale" -%}
  		{%- assign wholesale = true -%}
  	{%- endif -%}
  {%- endfor -%}

  {% if wholesale == true and template != 'cart' %}
  <script>
    $(function(){
      $(document).on('click', "button[name='add']", function(e){
        e.preventDefault();
        window.location = "/a/wsg/proxy/ws-cart";
      });
    });
  </script>
  {% endif %}

