{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/diaspora-co-spices.myshopify.com/products/configure/158115/edit
{% endcomment %}
{% if product.metafields.custom_fields["pre-order"] != blank %}
  <div class="custom-field custom-field__pre-order custom-field__type--checkbox">
    <h3 class="custom-field--title">Pre-Order</h3>
    <div class="custom-field--value">
      {% if product.metafields.custom_fields["pre-order"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}